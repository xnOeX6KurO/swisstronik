#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/swisstronik/node_modules/solc/node_modules:/workspaces/swisstronik/node_modules:/workspaces/node_modules:/node_modules:/workspaces/swisstronik/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/swisstronik/node_modules/solc/node_modules:/workspaces/swisstronik/node_modules:/workspaces/node_modules:/node_modules:/workspaces/swisstronik/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../solc/solc.js" "$@"
else
  exec node  "$basedir/../solc/solc.js" "$@"
fi
